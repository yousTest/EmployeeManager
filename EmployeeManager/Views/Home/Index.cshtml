@model IEnumerable<EmployeeManager.Models.Employee>

@{
    ViewBag.Title = "Index page";
    Layout = "../Shared/_Layout";
}

<table>
    <thead>
    <th>ID</th>
    <th>Name</th>
    <th>Department</th>
    </thead>
    <tbody>
        @foreach (var employee in Model)
        {
            <tr>
                <td>@employee.Id</td>
                <td>@employee.Name</td>
                <td>@employee.Department</td>
            </tr>
        }
    </tbody>

</table>